public class Solution {
    public int majorityElement(int[] nums) {
        int candidate = 0;
        int vote = 0;

        for(int num: nums){

            //if vote is 0 means it is new candidate
            if(vote==0){
                candidate = num;
            }
            if(num==candidate){
                vote++;
            }else{
                vote--;
            }
        }
        //which contains the majority element
        return candidate;
    }
}

//boymore vote algorithm/ Moore's Voting Algorithm:
//SC:O(1)
//TC:O(n)
[2,2,1,1,1,2,2]
o/p:2
